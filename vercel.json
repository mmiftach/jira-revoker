{
    "version": 2,
    "name": "jira-revoker",
    "builds": [
      {
        "src": "webserver.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.8" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "webserver.py"
      }
    ]
  }