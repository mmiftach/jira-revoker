{
    "version": 2,
    "name": "jira-revoker",
    "builds": [
      {
        "src": "webserver.py",
        "use": "@vercel/python"
      },
      {
        "src": "static/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "webserver.py"
      }
    ]
  }