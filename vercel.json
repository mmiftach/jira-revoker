{
    "version": 2,
    "name": "jira-revoker",
    "builds": [
      {
        "src": "webserver.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "webserver.py"
      }
    ]
  }